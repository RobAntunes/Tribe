{
  "name": "tribe",
  "displayName": "Tribe",
  "description": "Linting support for python files using `tribe`.",
  "version": "0.1.0",
  "preview": true,
  "serverInfo": {
    "name": "Tribe",
    "module": "tribe"
  },
  "publisher": "MightyDev",
  "license": "MIT",
  "homepage": "https://github.com/RobAntunes/Tribe",
  "repository": {
    "type": "git",
    "url": "https://github.com/RobAntunes/Tribe.git"
  },
  "bugs": {
    "url": "https://github.com/RobAntunes/Tribe/issues"
  },
  "galleryBanner": {
    "color": "#1e415e",
    "theme": "dark"
  },
  "keywords": [
    "python",
    "tribe"
  ],
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "AI"
  ],
  "extensionDependencies": [],
  "capabilities": {
    "virtualWorkspaces": {
      "supported": false,
      "description": "Virtual Workspaces are not supported with Tribe."
    }
  },
  "files": [
    "out",
    "src",
    "tribe/**/*.py",
    "bundled/**/*"
  ],
  "activationEvents": [
    "onLanguage:python",
    "workspaceContains:*.py"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "package": "npm run build && node scripts/prepackage.js",
    "build": "npm run build:extension && npm run build:webview",
    "build:extension": "webpack --config webpack.config.js",
    "build:webview": "webpack --config webview/webpack.config.js",
    "watch": "concurrently \"npm run watch:extension\" \"npm run watch:webview\"",
    "watch:extension": "webpack --watch --config webpack.config.js",
    "watch:webview": "webpack --watch --config webview/webpack.config.js",
    "test": "npm run test:extension && npm run test:webview && npm run test:python",
    "test:extension": "node ./out/test/runTest.js",
    "test:webview": "jest",
    "test:python": "python -m pytest tribe/tests"
  },
  "contributes": {
    "views": {
      "crew-ai": [
        {
          "type": "webview",
          "id": "tribe.crewPanel",
          "name": "Oversee"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "crew-ai",
          "title": "Tribe",
          "icon": "fire.svg"
        }
      ]
    },
    "configuration": {
      "properties": {
        "tribe.args": {
          "default": [],
          "description": "Arguments passed in. Each argument is a separate item in the array.",
          "items": {
            "type": "string"
          },
          "scope": "resource",
          "type": "array"
        },
        "tribe.path": {
          "default": [],
          "description": "When set to a path to tribe binary, extension will use that. NOTE: Using this option may slowdown server response time.",
          "scope": "resource",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "tribe.importStrategy": {
          "default": "useBundled",
          "description": "Defines where `tribe` is imported from. This setting may be ignored if `tribe.path` is set.",
          "enum": [
            "useBundled",
            "fromEnvironment"
          ],
          "enumDescriptions": [
            "Always use the bundled version of `tribe`.",
            "Use `tribe` from environment, fallback to bundled version only if `tribe` not available in the environment."
          ],
          "scope": "window",
          "type": "string"
        },
        "tribe.interpreter": {
          "default": [],
          "description": "When set to a path to python executable, extension will use that to launch the server and any subprocess.",
          "scope": "resource",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "tribe.showNotifications": {
          "default": "off",
          "description": "Controls when notifications are shown by this extension.",
          "enum": [
            "off",
            "onError",
            "onWarning",
            "always"
          ],
          "enumDescriptions": [
            "All notifications are turned off, any errors or warning are still available in the logs.",
            "Notifications are shown only in the case of an error.",
            "Notifications are shown for errors and warnings.",
            "Notifications are show for anything that the server chooses to show."
          ],
          "scope": "machine",
          "type": "string"
        }
      }
    },
    "commands": [
      {
        "title": "Restart Server",
        "category": "Tribe",
        "command": "tribe.restart"
      },
      {
        "command": "tribe.showCrewPanel",
        "title": "Show Crew AI Panel",
        "category": "Tribe"
      }
    ]
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.5",
    "@types/markdown-it": "^14.1.2",
    "@vscode/python-extension": "^1.0.5",
    "concurrently": "^9.1.2",
    "fs-extra": "^11.2.0",
    "lucide-react": "^0.475.0",
    "markdown-it": "^14.1.0",
    "ncp": "^2.0.0",
    "react-markdown": "^10.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.18.126",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/vscode": "^1.97.0",
    "@types/vscode-notebook-renderer": "^1.72.3",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^2.22.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.1",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.1.1",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.0.5",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "vscode-languageclient": "^9.0.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4"
  },
  "bundledExtensions": [
    "ms-python.python"
  ]
}
